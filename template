package main

import (
    "AdventOfCode2021/utils/files"
)

func main() {
    input := files.ReadFile(1)
    println(solvePart1(input))
    println(solvePart2(input))
}

func solvePart1(input []string) int {
    result := 0
    for idx, s := range input {
        println(idx, s)
    }
    return result
}

func solvePart2(input []string) int {
    result := 0
    for idx, s := range input {
        println(idx, s)
    }
    return result
}
